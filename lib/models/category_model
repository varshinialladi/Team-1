class Category {
  final String name;
  final String image;
  Category({required this.name, required this.image});
}

final List<Category> categories = [
  Category(name: 'Shoes', image: 'images/shoes.jpeg'),
  Category(name: 'Skincare', image: 'images/skincare.jpeg'),
  Category(name: 'Electronics', image: 'images/img1.jpeg'),
];